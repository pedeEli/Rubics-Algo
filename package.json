{
	"name": "rubicsalgo-v2",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui-light.css -i src/theme/light",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@mdi/js": "^7.0.96",
		"@smui-extra/accordion": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/dialog": "^6.0.0-beta.16",
		"@smui/fab": "^6.0.0",
		"@smui/snackbar": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-cloudflare": "^1.0.0-next.23",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"cypress": "^10.2.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"firebase": "^9.8.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^2.9.13",
		"yup": "^0.32.11"
	},
	"type": "module"
}
